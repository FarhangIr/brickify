@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ext: <https://example.com/schema/BrickExtension#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ext:Ballast a owl:Class ;
    rdfs:label "Ballast" ;
    rdfs:subClassOf brick:Lighting ;
    skos:definition "" .

ext:Lamp a owl:Class ;
    rdfs:label "Lamp" ;
    rdfs:subClassOf brick:Lighting ;
    skos:definition "" .

ext:Lighting_Zone_Equipment_Group a owl:Class ;
    rdfs:label "Lighting Zone Equipment Group" ;
    rdfs:subClassOf brick:Lighting_System ;
    skos:definition "A grouping of luminaires that are driven by common control logic" .

