@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ext: <https://example.com/schema/BrickExtension#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ext:Blind_Controller a owl:Class ;
    rdfs:label "Blind Controller" ;
    rdfs:subClassOf ext:Blind_Equipment ;
    skos:definition "A controller used to control electric motors in blinds, roller shutters, canopies, awnings and curtains" .

ext:Blind_Zone a owl:Class ;
    rdfs:label "Blind Zone" ;
    rdfs:subClassOf brick:Zone ;
    skos:definition "" .

ext:Blind_Zone_Equipment_Group a owl:Class ;
    rdfs:label "Blind Zone Equipment Group" ;
    rdfs:subClassOf ext:Blind_System ;
    skos:definition "A grouping of blind motors and controllers that are driven by common control logic" .

ext:Blind_Equipment a owl:Class ;
    rdfs:label "Blind Equipment" ;
    rdfs:subClassOf brick:Equipment ;
    skos:definition "" .

ext:Blind_System a owl:Class ;
    rdfs:label "Blind System" ;
    rdfs:subClassOf brick:System ;
    skos:definition "" .

