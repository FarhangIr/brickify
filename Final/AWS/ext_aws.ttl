@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ext: <https://example.com/schema/BrickExtension#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

ext:Average_Indoor_Environment_Quality_Sensor a owl:Class ;
    rdfs:label "Average Indoor Environment Quality Sensor" ;
    rdfs:subClassOf brick:Sensor ;
    skos:definition "" .

ext:Gateway a owl:Class ;
    rdfs:label "Gateway" ;
    rdfs:subClassOf ext:Environment_Quality_Equipment ;
    skos:definition "" .

ext:Indoor_Environment_Quality_Sensor a owl:Class ;
    rdfs:label "Indoor Environment Quality Sensor" ;
    rdfs:subClassOf brick:Sensor ;
    skos:definition "" .

ext:Room_Count a owl:Class ;
    rdfs:label "Room Count" ;
    rdfs:subClassOf brick:Sensor ;
    skos:definition "" .

ext:Sound_Pressure_Level_Sensor a owl:Class ;
    rdfs:label "Sound Pressure Level Sensor" ;
    rdfs:subClassOf brick:Sensor ;
    skos:definition "" .

ext:Environment_Quality_Equipment a owl:Class ;
    rdfs:label "Environment Quality Equipment" ;
    rdfs:subClassOf brick:Equipment ;
    skos:definition "" .

