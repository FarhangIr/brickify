@prefix bldg: <https://example.com/bldg#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix ext: <https://example.com/schema/BrickExtension#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unit: <http://qudt.org/vocab/unit/> .

bldg:X-T-LR-LIFT-A_BoardingCallsSide1 a ext:Elevator_Boarding_Call ;
    rdfs:label "Boarding Call Side 1" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_DestinationCallsSide1 a ext:Elevator_Destination_Call ;
    rdfs:label "Destination Call Side 1" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_DoorsStateSide1 a ext:Elevator_Door_State ;
    rdfs:label "Door State Side 1" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_GroupPosition a ext:Elevator_Group_Position ;
    rdfs:label "Group Position" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_Load a ext:Elevator_Load ;
    rdfs:label "Load" ;
    brick:hasUnit unit:PERCENT ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_MovingDirection a ext:Elevator_Moving_Direction ;
    rdfs:label "Moving Direction" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_OperationMode a ext:Elevator_Operation_Mode ;
    rdfs:label "Operation Mode" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_PassangerAlarm a ext:Elevator_Passanger_Alarm ;
    rdfs:label "Passanger Alarm" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A_TripCounter a ext:Elevator_Trip_Counter ;
    rdfs:label "Trip Counter" ;
    brick:isPointOf bldg:X-T-LR-LIFT-A .

bldg:X-T-LR-LIFT-A a brick:Elevator ;
    rdfs:label "X-T-LR-LIFT-A" ;
    brick:hasLocation bldg:ES_A .

